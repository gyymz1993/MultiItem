<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.freelib.multiitem.demo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MultiItemActivity"/>
        <activity android:name=".ItemClickActivity"/>
        <activity android:name=".ChatActivity"/>
        <activity android:name=".HeadFootActivity"/>
        <activity android:name=".FullSpanGridActivity"/>
        <activity android:name=".LoadMoreActivity"/>
        <activity android:name=".PanelActivity"/>
        <activity android:name=".DataBindActivity"/>
        <activity android:name=".InputActivity"/>
        <activity android:name=".UserInfoActivity"/>
        <activity android:name=".EmptyAndErrorActivity"/>
        <activity android:name=".AnimationActivity"/>
    </application>

</manifest>